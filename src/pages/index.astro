---
import Layout from "../layouts/Layout.astro";
import {
  Navbar,
  HeroSection,
  AboutSection,
  ProjectSection,
} from "../components";

const navList = [
  { key: "nav-01", text: "About", href: "#about" },
  { key: "nav-02", text: "Project", href: "#project" },
];
---

<Layout title="EZ Portfolio">
  <Navbar navList={navList} />
  <HeroSection href="#about" />
  <AboutSection id="#about" />
  <ProjectSection />
</Layout>
